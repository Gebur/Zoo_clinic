<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9E85BDF1"
    tools:context=".dantists">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="139dp"
        android:layout_height="137dp"
        android:layout_marginStart="135dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/green" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/resurs_6" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="412dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:background="#E5C390"
        android:padding="12dp"
        android:text="@string/dantists"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/doctorName1"
        android:layout_width="412dp"
        android:layout_height="57dp"
        android:layout_marginTop="44dp"
        android:background="#B39BC8"
        android:text="@string/dantsit1"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/doctorName2"
        android:layout_width="412dp"
        android:layout_height="57dp"
        android:layout_marginTop="28dp"
        android:background="#B39BC8"
        android:text="Борисов Б.Б."
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctorName1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/doctorName3"
        android:layout_width="412dp"
        android:layout_height="57dp"
        android:layout_marginTop="28dp"
        android:background="#B39BC8"
        android:text="Кирилов К.К."
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctorName2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/dantBack"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="32dp"
        android:background="#E5C390"
        android:text="Назад"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctorName3" />

</androidx.constraintlayout.widget.ConstraintLayout>